      "source": [
        "def trade(prices):\n",
        "    buy_price = min(prices)\n",
        "    sell_price = max(prices[prices.index(buy_price):])\n",
        "    return buy_price, sell_price\n",
        "\n",
        "prices = [5, 2, 1, 6, 9, 7]\n",
        "buy, sell = trade(prices)\n",
        "print(trade(prices))\n",
        "print(\"Best Buy Price:\", buy)\n",
        "print(\"Best Sell Price:\", sell)"
